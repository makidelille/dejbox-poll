{
    "name": "Dejbox-Poll",
    "version": "1.0",
    "description": "Extension dejbox",
    "permissions": ["storage"],
    "content_scripts": [
        {
          "matches": ["http://*.dejbox.fr/*", "https://*.dejbox.fr/*"],
          "js": ["contentscript.js"]
        }
      ],
    "options_page": "options.html",
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "manifest_version": 2
  }